<% if(message) { %>
  <script>
  $(function() {

    var progressed = <%= percentage %>; 
  
    var interval = setInterval(function() {
    
        $("#prog")
    
        .css("width", progressed + "%")
    
        .attr("aria-valuenow", progressed)
    
        .text(progressed + "%");
    
        if (progressed >= 100)
    
            clearInterval(interval);
    
    }, 100);
  });
  document.getElementById("header").innerHTML = "battery level = "  + <%= percentage %>;
  document.getElementById("windspeed").innerHTML = <%= windspeed %> + "m/s";
  document.getElementById("production").innerHTML = <%= production %> + "Kwh";
  document.getElementById("consumption").innerHTML = <%= consumption %> + "Kwh";
  document.getElementById("netproduction").innerHTML = <%= netproduction %>+ "Kwh";
  document.getElementById("battery").innerHTML = <%= battery %> + "kwh";
  document.getElementById("electricityprice").innerHTML = <%= electricityprice %> + "kr/kwh";
  </script>
 <%}%>

 </html>